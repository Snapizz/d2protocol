{
  "compilerOptions": {
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./lib",
    "preserveConstEnums": true,
    "removeComments": true,
    "target": "ES5"
  },
  "filesGlob": [
    "src/protocol/**/*.ts",
    "src/*.ts"
  ],
  "files": [
    "src/protocol.ts"
  ]
}